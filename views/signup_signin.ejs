<%- include('partials/header') %>


    <div class="susi_container">
        <div class="s_container">
            <div class="form_container">
                <div class="signin_signup">
                    <form action="" class="sign_in_form">
                        <h2 class="title">Sign In</h2>
                        <div class="socials">
                            <a href="/" class="s_social_icon">
                                <i class="fab fa-facebook-f"></i>
                            </a>
                            <a href="/" class="s_social_icon">
                                <i class="fab fa-google"></i>
                            </a>
                            <a href="/" class="s_social_icon">
                                <i class="fab fa-linkedin-in"></i>
                            </a>
                        </div>
                        <p class="social_text">Or Sign in with Email</p>
                        <div class="input_field"><i class="fas fa-user"></i>
                            <input type="text" placeholder="Email">
                        </div>
                        <div class="input_field"><i class="fas fa-lock"></i>
                            <input type="text" placeholder="Password">
                        </div>
                        <input type="submit" value="login" class="s_btn solid">
                    </form>

                    <form action="" class="sign_up_form">
                        <h2 class="title">Sign Up</h2>
                        <div class="socials">
                            <a href="/" class="s_social_icon">
                                <i class="fab fa-facebook-f"></i>
                            </a>
                            <a href="/" class="s_social_icon">
                                <i class="fab fa-google"></i>
                            </a>
                            <a href="/" class="s_social_icon">
                                <i class="fab fa-linkedin-in"></i>
                            </a>
                        </div>
                        <p class="social_text">Or Sign up with Email</p>
                        <div class="input_field"><i class="fas fa-user"></i>
                            <input type="text" placeholder="Email">
                        </div>
                        <div class="input_field"><i class="fas fa-lock"></i>
                            <input type="text" placeholder="Password">
                        </div>
                        <div class="input_field"><i class="fas fa-lock"></i>
                            <input type="text" placeholder="Confirm Password">
                        </div>
                        <input type="submit" value="Sign Up" class="s_btn solid">
                    </form>
                </div>
            </div>

            <div class="panels_container">
                <div class="panel left_panel">
                    <div class="content">
                        <h3>Don't Have An Account?</h3>
                        <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Voluptatem magni fugit labore.</p>
                        <button class="s_btn transparent" id="sign_up_btn">Sign Up</button>
                    </div>
                    <!-- <img src="/images/logins.svg" class="sp_images" alt=""> -->
                </div>

                <div class="panel right_panel">
                    <div class="content">
                        <h3>Already a Member?</h3>
                        <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Voluptatem magni fugit labore.</p>
                        <button class="s_btn transparent" id="sign_in_btn">Sign In</button>
                    </div>
                    <!-- <img src="/images/signups.svg" class="sp_images" alt=""> -->
                </div>
            </div>
        </div>
    </div>


    <%- include('partials/footer') %>